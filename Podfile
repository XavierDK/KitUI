# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Demo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  # Pods for Demo
  
  pod 'NeoCoordinator'
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'Action'
  pod 'Reusable'

end

target 'KitUI' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for KitUI

  target 'KitUITests' do
    inherit! :search_paths
    
    # Pods for testing
    
    pod 'Quick'
    pod 'Nimble'    
    
  end

end
